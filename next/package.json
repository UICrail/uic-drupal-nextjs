{
  "name": "next-drupal-starterkit",
  "version": "0.1.0",
  "description": "Next-drupal template by Wunder",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --raw 'npm run graphql-codegen:watch' 'npm run dev:nocodegen'",
    "dev:nocodegen": "next dev",
    "build": "npm run graphql-codegen && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "build-storybook": "STORYBOOK=true storybook build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "codegen-access-token": "ts-node ./codegen-access-token.ts",
    "graphql-codegen": "npm run codegen-access-token && graphql-codegen --config codegen.ts",
    "graphql-codegen:watch": "npm run codegen-access-token && graphql-codegen  --config codegen.ts --watch"
  },
  "dependencies": {
    "@neshca/cache-handler": "^1.7.3",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@searchkit/api": "^4.13.0",
    "@searchkit/instantsearch-client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@tanstack/react-query": "^5.51.11",
    "@tanstack/react-query-devtools": "^5.51.11",
    "clsx": "^2.1.1",
    "cva": "npm:class-variance-authority@0.7.0",
    "drupal-jsonapi-params": "^2.3.1",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "html-react-parser": "^5.1.12",
    "instantsearch.css": "^8.5.0",
    "jwt-decode": "^4.0.0",
    "next": "^14.2.5",
    "next-auth": "^5.0.0-beta.20",
    "next-drupal": "^2.0.0-beta.0",
    "next-intl": "^3.19.1",
    "next-seo": "^6.5.0",
    "p-retry": "^6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-instantsearch": "^7.12.0",
    "react-youtube": "^10.1.0",
    "redis": "^4.7.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "@parcel/watcher": "^2.4.1",
    "@storybook/addon-essentials": "^8.2.5",
    "@storybook/addon-interactions": "^8.2.5",
    "@storybook/addon-links": "^8.2.5",
    "@storybook/blocks": "^8.2.5",
    "@storybook/nextjs": "^8.2.5",
    "@storybook/react": "^8.2.5",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cypress": "^13.14.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "storybook": "^8.2.5",
    "storybook-next-intl": "^1.1.4",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "next-drupal": {
      "next": "$next"
    }
  }
}
